<launch>
  <!-- 차선 중심 제어 노드 -->
  <node pkg="autorace_decision"
        type="controller_lane_logic"
        name="mission_lane"
        output="screen">

    <!-- 이 노드의 private 파라미터(~)로 YAML 로드 -->
    <rosparam command="load"
              file="$(find autorace_control)/config/controller_lane_logic.yaml" />
  </node>

  <!-- 차선 인식(perception) 노드: rosrun autorace_perception lane_kante_wego -->
  <node pkg="autorace_perception"
        type="lane_kante_wego"
        name="lane_kante_wego"
        output="screen" />
</launch>
